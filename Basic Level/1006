#include <iostream>
#include<string>
using namespace std;

int main()
{
    int a=0,b;
    string out1,out2,out3;
    cin>>a;
    for (int i=1;i<=a%10;i++)
        out3+=i+'0';
    a=a/10;
    for (int i=1;i<=a%10;i++)
        out2+='S';
    a=a/10;
    for (int i=1;i<=a%10;i++)
        out1+='B';
    out1+=out2+out3;
    cout<<out1;
    return 0;
}
